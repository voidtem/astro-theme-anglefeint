{
  "name": "@anglefeint/astro-theme",
  "version": "0.1.2",
  "type": "module",
  "description": "Anglefeint core theme package for Astro",
  "license": "MIT",
  "files": [
    "src/index.ts",
    "src/consts.ts",
    "src/content-schema.ts",
    "src/components",
    "src/layouts",
    "src/i18n",
    "src/styles",
    "src/assets/theme",
    "src/cli-new-post.mjs",
    "src/cli-new-page.mjs",
    "public"
  ],
  "bin": {
    "anglefeint-new-post": "src/cli-new-post.mjs",
    "anglefeint-new-page": "src/cli-new-page.mjs"
  },
  "exports": {
    ".": "./src/index.ts",
    "./components/*": "./src/components/*",
    "./layouts/*": "./src/layouts/*",
    "./i18n/*": "./src/i18n/*",
    "./styles/*": "./src/styles/*",
    "./assets/*": "./src/assets/*",
    "./public/*": "./public/*",
    "./content-schema": "./src/content-schema.ts",
    "./consts": "./src/consts.ts"
  },
  "peerDependencies": {
    "astro": "^5.0.0"
  }
}
