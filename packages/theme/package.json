{
  "name": "@anglefeint/astro-theme",
  "version": "0.1.16",
  "type": "module",
  "description": "Anglefeint core theme package for Astro",
  "keywords": [
    "astro",
    "astro-theme",
    "blog",
    "portfolio",
    "cyberpunk",
    "hacker",
    "matrix"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=22.12.0"
  },
  "files": [
    "src/index.ts",
    "src/consts.ts",
    "src/content-schema.ts",
    "src/config",
    "src/components",
    "src/layouts",
    "src/scripts",
    "src/i18n",
    "src/styles",
    "src/assets/theme",
    "src/cli-new-post.mjs",
    "src/cli-new-page.mjs"
  ],
  "bin": {
    "anglefeint-new-post": "src/cli-new-post.mjs",
    "anglefeint-new-page": "src/cli-new-page.mjs"
  },
  "exports": {
    ".": "./src/index.ts",
    "./components/*": "./src/components/*",
    "./layouts/*": "./src/layouts/*",
    "./scripts/*": "./src/scripts/*",
    "./i18n/*": "./src/i18n/*",
    "./styles/*": "./src/styles/*",
    "./assets/*": "./src/assets/*",
    "./content-schema": "./src/content-schema.ts",
    "./consts": "./src/consts.ts"
  },
  "peerDependencies": {
    "astro": "^5.0.0 || ^6.0.0-beta.0"
  }
}
